  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        r.radnik_ID, o.dtm_odrzavanja, ROUND(o.odrzavanje_trosak, 2) AS 'Troškovi održavanja', vp.vozilo_tip_naziv
FROM            radnik AS r INNER JOIN
                         odrzavanje AS o ON o.radnik_ID = r.radnik_ID INNER JOIN
                         vozilo AS v ON v.vozilo_ID = o.vozilo_ID INNER JOIN
                         vozilo_tip AS vp ON vp.vozilo_tip_ID = v.vozilo_tip_ID
WHERE        (vp.vozilo_tip_naziv IN ('minibus', 'kombibus'))</CommandText>
      </Query>
      <Fields>
        <Field Name="radnik_ID">
          <DataField>radnik_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dtm_odrzavanja">
          <DataField>dtm_odrzavanja</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Troškovi_održavanja">
          <DataField>Troškovi održavanja</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="vozilo_tip_naziv">
          <DataField>vozilo_tip_naziv</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
